{
  "name": "colonial-ui",
  "version": "3.1.7",
  "license": "UNLICENSED",
  "keywords": [
    "colonial",
    "angular",
    "ng",
    "ui"
  ],
  "contributors": [
    "Chariot Solutions, LLC"
  ],
  "bugs": {
    "url": "https://colonial.atlassian.net/projects/PL/issues/?filter=allopenissues"
  },
  "homepage": "https://colonial.atlassian.net/secure/Dashboard.jspa",
  "author": "Chariot Solutions",
  "description": "Next Gen Bonds SPA UI using angular 6 and material design",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/colonial/colonial-ui"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --aot --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check --live-reload --progress ",
    "start:prod": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check --live-reload --progress --aot --optimization ---prod",
    "build": "ng build --aot --named-chunks --fork-type-checker --show-circular-dependencies",
    "build:verbose": "ng build --aot --verbose --progress=false --named-chunks --fork-type-checker --show-circular-dependencies ",
    "build:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration production --base-href=/quote/ --deploy-url=/quote/",
    "build-dev:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build --configuration production --base-href=/quote/ --deploy-url=/quote/",
    "test": "ng test --source-map=true --code-coverage",
    "test:ci": "ng test --source-map=false --progress=false --karma-config=src/karma.ci.conf.js",
    "lint": "ng lint colonial --type-check --format verbose && ng lint colonial-e2e --type-check --format verbose",
    "lint:fix": "ng lint colonial --type-check --format verbose --fix && ng lint colonial-e2e --type-check --format verbose --fix",
    "format": "tsfmt -r",
    "format:verify": "tsfmt -r --verify",
    "e2e": "false",
    "clean": "rimraf coverage && rimraf test-reports && rimraf dist",
    "validate-package": "pjv -w -r",
    "preflight": "npm run validate-package && npm run lint && npm run format:verify && npm run test:ci && npm run build",
    "dammit": "npm run clean && rimraf package-lock.json && rimraf node_modules && npm install"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/flex-layout": "7.0.0-beta.24",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@types/lodash": "^4.14.104",
    "angular-user-idle": "^2.2.2",
    "angular2-material-datepicker": "^0.5.0",
    "bank-routing-number-validator": "^0.0.4",
    "bootstrap": "^4.3.1",
    "card-validator": "^6.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "json2typescript": "1.1.1",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "moment-range": "^4.0.1",
    "ng2-file-required": "^1.0.10",
    "ngx-device-detector": "1.4.5",
    "node-sass": "^4.14.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.0",
    "quill": "^1.3.6",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "uuid": "^3.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-optimizer": "^0.13.0",
    "@angular-devkit/core": "^7.0.7",
    "@angular-devkit/schematics": "^7.0.7",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.4",
    "@angular/language-service": "~7.1.4",
    "@types/googlemaps": "^3.39.4",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "3.3.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^10.12.10",
    "codelyzer": "~4.5.0",
    "copy-webpack-plugin": "4.6.0",
    "figlet": "^1.2.1",
    "jasmine": "^3.3.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasminewd2": "^2.2.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-parallel": "^0.3.1",
    "node-dev": "^3.1.3",
    "package-json-validator": "^0.6.3",
    "protractor": "~5.4.1",
    "rimraf": "~2.6.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "typescript-formatter": "^7.2.2"
  }
}
